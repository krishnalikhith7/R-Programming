print("enter the values in a vector") x<- scan()
 
n<- length(x) for(i in 1: (n-1))
for(j in (i+1) : n)
if(x[i]>x[j])
{
temp<- x[i] x[i]<- x[j] x[j]<- temp
}
print(x)
range<- x[n]-x[1]
print("range of given data is") print(range)
print("range of given data using built in function is") print(range(x))
